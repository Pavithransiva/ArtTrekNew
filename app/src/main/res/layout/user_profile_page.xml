<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainBlack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/imageView6"
                    android:layout_width="match_parent"
                    android:layout_height="450dp"
                    android:backgroundTint="#505050"
                    android:backgroundTintMode="screen"
                    android:scaleType="centerCrop"
                    android:src="@drawable/img_topcollection_4"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/button14"
                        style="@style/Widget.MaterialComponents.Button.Icon"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginTop="300dp"
                        android:insetLeft="0dp"
                        android:insetTop="0dp"
                        android:insetRight="0dp"
                        android:insetBottom="0dp"
                        android:padding="0dp"
                        app:backgroundTint="@color/mainLightGrey"
                        app:icon="@drawable/ic_baseline_add_24"
                        app:iconGravity="textStart"
                        app:iconPadding="0dp"
                        app:iconSize="30dp"
                        app:iconTint="@color/mainBlack"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.CornerSize50Percent" />

                    <TextView
                        android:id="@+id/textView16"
                        style="@style/artistpage_artistname_textstyle"
                        android:text="TextView"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/button14" />

                    <TextView
                        android:id="@+id/textView24"
                        style="@style/artistpage_artisttitle_textstyle"
                        android:text="TextView"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView16" />

                    <TextView
                        android:id="@+id/textView41"
                        style="@style/artistpage_follownumber_textstyle"
                        android:text="100"
                        android:textAlignment="center"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView16" />

                    <TextView
                        android:id="@+id/textView100"
                        style="@style/artistpage_followtext_textstyle"
                        android:text="Following"
                        android:textAlignment="center"
                        app:layout_constraintEnd_toEndOf="@+id/textView41"
                        app:layout_constraintTop_toBottomOf="@+id/textView41" />

                    <TextView
                        android:id="@+id/textView101"
                        style="@style/artistpage_followtext_textstyle"
                        android:text="Followers"
                        android:textAlignment="center"
                        app:layout_constraintEnd_toEndOf="@+id/textView102"
                        app:layout_constraintTop_toBottomOf="@+id/textView102" />

                    <TextView
                        android:id="@+id/textView102"
                        style="@style/artistpage_follownumber_textstyle"
                        android:layout_marginRight="15dp"
                        android:text="100"
                        android:textAlignment="center"
                        app:layout_constraintEnd_toStartOf="@+id/textView41"
                        app:layout_constraintTop_toBottomOf="@+id/textView16" />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/button15"
                        style="@style/Widget.MaterialComponents.Button.Icon"
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:insetLeft="0dp"
                        android:insetTop="2dp"
                        android:insetRight="0dp"
                        android:insetBottom="2dp"
                        android:text="Button"
                        android:textAppearance="@style/artistpage_button_textstyle"
                        android:textColor="@color/mainBlack"
                        app:backgroundTint="@color/mainLightGrey"
                        app:icon="@drawable/ic_baseline_chat_bubble_24"
                        app:iconGravity="textEnd"
                        app:iconPadding="10dp"
                        app:iconTint="@color/mainBlack"
                        app:layout_constraintEnd_toStartOf="@+id/button16"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView24" />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/button16"
                        style="@style/Widget.MaterialComponents.Button.Icon"
                        android:layout_width="180dp"
                        android:layout_height="wrap_content"
                        android:insetLeft="0dp"
                        android:insetTop="2dp"
                        android:insetRight="0dp"
                        android:insetBottom="2dp"
                        android:text="Button"
                        android:textAppearance="@style/artistpage_button_textstyle"
                        android:textColor="@color/mainBlack"
                        app:backgroundTint="@color/mainLightGrey"
                        app:icon="@drawable/ic_baseline_chat_bubble_24"
                        app:iconGravity="textEnd"
                        app:iconPadding="10dp"
                        app:iconTint="@color/mainBlack"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/button15"
                        app:layout_constraintTop_toTopOf="@+id/button15" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayout5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/button15">

                        <com.google.android.material.chip.ChipGroup
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <com.google.android.material.chip.Chip
                                android:id="@+id/chip2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Painting"
                                android:textAppearance="@style/artistpage_chip_textstyle"
                                android:textColor="@color/mainLightGrey"
                                app:chipBackgroundColor="@color/mainOrange" />

                        </com.google.android.material.chip.ChipGroup>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayout6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayout5">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/SVHP0_LL0_constraintLayout_2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="10dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/SVHP0_LL0_CL2_textView_0"
                                style="@style/HomePage_Title_Style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/Title_3"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/SVHP0_LL0_CL2_textView_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="TextView"
                                android:textColor="@color/mainYellow"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <HorizontalScrollView
                            android:id="@+id/SVHP0_LL0_horizontalScrollView_3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_constraintLayout_2">

                            <LinearLayout
                                android:id="@+id/SVHP0_LL0_HSV3_linearLayout_0"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/SVHP0_LL0_HSV3_LL0_constraintLayout_0"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <com.google.android.material.imageview.ShapeableImageView
                                        android:id="@+id/SVHP0_LL0_HSV3_LL0_CL0_imageView_0"
                                        style="@style/image_container_dimension_featureArtwork"
                                        android:scaleType="centerCrop"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:shapeAppearanceOverlay="@style/container_roundedCorner_topcollection"
                                        app:srcCompat="@drawable/img_topcollection_4" />

                                    <TextView
                                        android:id="@+id/SVHP0_LL0_HSV3_LL0_CL0_textView_0"
                                        style="@style/FeaturedArtwork_Subtitle_Style"
                                        android:text="TextView"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_HSV3_LL0_CL0_imageView_0" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </HorizontalScrollView>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayout7"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayout6">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/SVHP0_LL0_constraintLayout_3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/SVHP0_LL0_textView_1"
                                style="@style/HomePage_Title_Style"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/Title_4"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/textView99"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="TextView"
                                android:textColor="@color/mainYellow"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <HorizontalScrollView
                            android:id="@+id/SVHP0_LL0_horizontalScrollView_4"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_constraintLayout_3">

                            <LinearLayout
                                android:id="@+id/SVHP0_LL0_HSV4_linearLayout_0"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/SVHP0_LL0_HSV4_LL0_constraintLayout_0"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <com.google.android.material.imageview.ShapeableImageView
                                        android:id="@+id/SVHP0_LL0_HSV4_LL0_CL0_imageView_0"
                                        style="@style/image_container_dimension_featureArtwork"
                                        android:scaleType="centerCrop"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:shapeAppearanceOverlay="@style/container_roundedCorner_topcollection"
                                        app:srcCompat="@drawable/img_topcollection_4" />

                                    <TextView
                                        android:id="@+id/SVHP0_LL0_HSV4_LL0_CL0_textView_0"
                                        style="@style/FeaturedArtwork_Subtitle_Style"
                                        android:text="TextView"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_HSV4_LL0_CL0_imageView_0" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </HorizontalScrollView>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="20dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayout7">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/SVHP0_LL0_constraintLayout_0"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/SVHP0_LL0_CL0_textView_0"
                                style="@style/HomePage_Title_Style"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/Title_1"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/SVHP0_LL0_CL0_textView_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="TextView"
                                android:textColor="@color/mainYellow"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <HorizontalScrollView
                            android:id="@+id/SVHP0_LL0_horizontalScrollView_1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="20dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_constraintLayout_0">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/SVHP0_LL0_HSV1_LL0_constraintLayout_0"
                                android:layout_width="200dp"
                                android:layout_height="match_parent">

                                <com.google.android.material.imageview.ShapeableImageView
                                    android:id="@+id/SVHP0_LL0_HSV1_LL0_CL0_imageView_0"
                                    style="@style/container_roundedCorner_categories_dimension"
                                    android:scaleType="centerCrop"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:shapeAppearanceOverlay="@style/container_roundedCorner_topcollection"
                                    app:srcCompat="@drawable/img_topcollection_4" />

                                <TextView
                                    android:id="@+id/SVHP0_LL0_HSV1_LL0_CL0_textView_0"
                                    style="@style/TopCollection_Subtitle_Style"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="TextView"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_HSV1_LL0_CL0_imageView_0" />

                                <TextView
                                    android:id="@+id/SVHP0_LL0_HSV1_LL0_CL0_textView_1"
                                    style="@style/TopCollection_Subtitle_Style_smaller"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="TextView"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_HSV1_LL0_CL0_textView_0" />

                                <TextView
                                    android:id="@+id/SVHP0_LL0_HSV1_LL0_CL0_textView_2"
                                    style="@style/TopCollection_Subtitle_Style_snaller_bold"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="TextView"
                                    app:layout_constraintStart_toEndOf="@+id/SVHP0_LL0_HSV1_LL0_CL0_textView_1"
                                    app:layout_constraintTop_toBottomOf="@+id/SVHP0_LL0_HSV1_LL0_CL0_textView_0" />

                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </HorizontalScrollView>
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>